    <!-- DITA
         the mimetype application/dita+xml is not formally registered with IANA
         see https://dita.fluidtopics.com/r/Darwin-Information-Typing-Architecture-DITA-Version-1.2/DITA-MIME-type
    -->
    <mime-type name="application/dita+xml" type="xml">
        <description>DITA document</description>
        <extensions>.dita,.ditamap,.ditaval</extensions>
    </mime-type>


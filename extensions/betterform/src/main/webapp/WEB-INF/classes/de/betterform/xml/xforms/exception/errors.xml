    <error id="xforms-binding-exception">
        <message xml:lang="de">
            <title>Ungültiger Binding Ausdruck</title>
            <description>
            Ein xforms:model- oder xforms:bind Attribute enthält eine ungültige Referenz.
            </description>
        </message>
        <message xml:lang="en">
            <title>Invalid Binding</title>
            <description>
            Dispatched as an indication of: an illegal binding expression,

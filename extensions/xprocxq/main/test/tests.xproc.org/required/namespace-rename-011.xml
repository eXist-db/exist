<?xml-stylesheet type="text/xsl" href="/style/testcase.xsl"?>
<t:test xmlns:t="http://xproc.org/ns/testsuite"
        xmlns:p="http://www.w3.org/ns/xproc"
        xmlns:c="http://www.w3.org/ns/xproc-step"
        xmlns:err="http://www.w3.org/ns/xproc-error">

  <t:title>Test p:namespace-rename #011</t:title>

  <t:input port="source">
    <d:doc xmlns:d="http://bar.com" bar:attr1="val1" x:attr2="val2" xmlns:x="http://test.com" xmlns:bar="http://bar.com">
      <p/>
      <p:p xmlns:p="http://bar.com"/>
    </d:doc>
  </t:input>

  <t:pipeline>
    <p:pipeline>
      <p:namespace-rename from="http://bar.com" apply-to="elements"/>
    </p:pipeline>
  </t:pipeline>

  <t:output port="result">
    <doc bar:attr1="val1" x:attr2="val2" xmlns:x="http://test.com" xmlns:bar="http://bar.com">
      <p/>
      <p/>
    </doc>
  </t:output>

</t:test>


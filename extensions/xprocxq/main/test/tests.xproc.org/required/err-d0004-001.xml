        xmlns:err="http://www.w3.org/ns/xproc-error"
	error="err:XD0004">

<t:title>Test err:XD0004</t:title>

<t:input port='source'>
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>Some title</title>
  </head>
  <body>
    <h1>Some title</h1>
    <p>Some <del>deleted</del>text.</p>

<?xml version="1.0" encoding="UTF-8"?>
<build>
    <!--  This file lists the metadata storage backends which are available to eXist.
        It is used to generate the build scripts. For a metadata storage to be build, 
        it needs to be enabled in build.properties. To add a new type of backend, 
        just copy the directory structure of an existing index and add a 
        new backend element below. -->
    <backends>
        <!-- sleepycat -->
        <backend name="sleepycat" dir="sleepycat" antfile="build.xml"/>
    </backends>
</build>

<processing>
    <logfiledir>$INSTALL_PATH</logfiledir>
    <job name="Set password for admin user ...">
        <os family="windows"/>
        <executefile name="$INSTALL_PATH/bin/setup.bat">
            <arg>$adminPasswd</arg>
        </executefile>
    </job>
    <job name="Set password for admin user ...">
        <os family="unix"/>
        <executefile name="$INSTALL_PATH/bin/setup.sh">
            <arg>pass=$adminPasswd</arg>
            <arg>doc=$apps.doc</arg>
            <arg>demo=$apps.demo</arg>
            <arg>fundocs=$apps.fundocs</arg>
        </executefile>
    </job>
</processing>
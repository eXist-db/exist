<?xml version="1.0" encoding="UTF-8"?>
<shortcuts>
	<programGroup defaultName="eXist-db XML Database" location="applications"/>
	
	<shortcut 
		name="org-exist-start-Main"
		genericName="eXist-db"
		type="Application"
		encoding="UTF-8"
		target="$JAVA_HOME/bin/java"
		commandLine="-jar start.jar"
		workingDirectory="$INSTALL_PATH"
		description="eXist-db XML Database Launcher" 
		programGroup="yes"
		desktop="yes"
		terminal="false"
		startupNotify="true"
		iconFile="$INSTALL_PATH/icon.png"/>
	<shortcut name="Uninstall eXist-db"
		encoding="UTF-8"
		target="$JAVA_HOME/bin/java"
		workingDirectory="$INSTALL_PATH"
		commandLine="-jar &quot;$INSTALL_PATH/uninstall.jar&quot;"
		iconFile="trashcan_full"
		description="Uninstall eXist-db" 
		programGroup="yes"
		desktop="no"
		type="Application"/>
</shortcuts>
